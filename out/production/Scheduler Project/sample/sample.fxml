<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<GridPane fx:controller="sample.Controller"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">

    <Label text="Enter The Process Number" GridPane.rowIndex="0" GridPane.columnIndex="0" textFill="darkorchid"/>
    <Label text="Enter The Arrival Time" GridPane.rowIndex="0" GridPane.columnIndex="1" textFill="darkorchid"/>

    <TextField GridPane.rowIndex="1" GridPane.columnIndex="0" prefWidth="180"/>
    <TextField GridPane.rowIndex="1" GridPane.columnIndex="1" />

    <VBox GridPane.rowIndex="3" spacing="10">
        <HBox spacing="20">
            <Label text="Enter Number Of Processes"/>
            <TextField />
        </HBox>
        <HBox>
            <Label text="Enter The Arrival Time   "/>
            <TextField />
        </HBox>
        <HBox>
            <Label text="Enter The Time Of Processes"/>
            <TextField />
        </HBox>
    </VBox>



</GridPane>